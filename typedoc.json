{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./src/index.ts"],
  "out": "./docs/api",
  "name": "@pulse8-ai/chat",
  "readme": "./README.md",
  "includeVersion": true,
  "categorizeByGroup": true,
  "categoryOrder": [
    "Configuration",
    "Components",
    "Hooks",
    "Adapters",
    "Utilities",
    "Types",
    "*"
  ],
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "sort": ["source-order", "alphabetical"],
  "exclude": [
    "**/node_modules/**",
    "**/*.stories.tsx",
    "**/*.test.ts",
    "**/*.test.tsx"
  ],
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "gitRevision": "main",
  "plugin": [],
  "theme": "default"
}
